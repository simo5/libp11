HOME			= .

 # Use this in order to automatically load providers.
openssl_conf = openssl_init

config_diagnostics = 1

[openssl_init]
providers = provider_sect

[provider_sect]
default = default_sect
pkcs11 = pkcs11_sect

[default_sect]
activate = 1

[pkcs11_sect]
module = @PROVIDER_PATH@
pkcs11-module-path = @MODULE_PATH@
activate = 1
